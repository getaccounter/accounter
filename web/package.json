{
  "name": "web",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@apollo/client": "3.3.5",
    "@tailwindcss/forms": "0.2.1",
    "@testing-library/jest-dom": "5.11.6",
    "@testing-library/react": "11.2.2",
    "@testing-library/user-event": "12.5.0",
    "@types/jest": "26.0.19",
    "@types/js-cookie": "2.2.6",
    "@types/node": "14.14.12",
    "@types/react": "17.0.0",
    "@types/react-dom": "17.0.0",
    "@types/react-router-dom": "5.1.6",
    "@types/uuid": "8.3.0",
    "graphql": "15.4.0",
    "heroicons": "0.4.2",
    "js-cookie": "2.2.1",
    "postcss": "8.2.1",
    "react": "17.0.1",
    "react-click-away-listener": "1.4.4",
    "react-dom": "17.0.1",
    "react-router-dom": "5.2.0",
    "react-scripts": "4.0.1",
    "styled-components": "5.2.1",
    "tailwindcss-cli": "0.1.2",
    "typescript": "4.0.5",
    "use-http": "1.0.16",
    "uuid": "8.3.2"
  },
  "scripts": {
    "build:tailwind": "tailwindcss-cli build src/tailwind.css -o src/tailwind.output.css",
    "prestart": "npm run build:tailwind",
    "prebuild": "npm run build:tailwind",
    "start": "react-scripts start",
    "build": "react-scripts build",
    "lint": "eslint --ext .js --ext .tsx --max-warnings 0 src/",
    "test": "react-scripts test",
    "typecheck": "tsc --noEmit -p tsconfig.json",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "jest": {
    "collectCoverageFrom": [
      "**/*.{js,jsx,ts,tsx}",
      "!**/node_modules/**",
      "!**/vendor/**",
      "!**/serviceWorker.ts",
      "!**/*.d.ts",
      "!src/index.tsx",
      "!src/App.tsx",
      "!src/**/*.mocks.ts*"
    ]
  },
  "devDependencies": {
    "@types/faker": "5.1.5",
    "factory": "https://registry.npmjs.org/factory.ts/-/factory.ts-0.5.1.tgz",
    "faker": "5.1.0"
  }
}
