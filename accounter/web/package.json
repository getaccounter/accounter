{
  "name": "web",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@apollo/client": "3.3.6",
    "@tailwindcss/forms": "0.2.1",
    "@testing-library/jest-dom": "5.11.9",
    "@testing-library/react": "11.2.3",
    "@testing-library/user-event": "12.6.0",
    "@types/jest": "26.0.20",
    "@types/js-cookie": "2.2.6",
    "@types/node": "14.14.21",
    "@types/react": "17.0.0",
    "@types/react-dom": "17.0.0",
    "@types/react-router-dom": "5.1.7",
    "@types/uuid": "8.3.0",
    "heroicons": "0.4.2",
    "js-cookie": "2.2.1",
    "postcss": "8.2.2",
    "query-string": "6.13.8",
    "react": "17.0.1",
    "react-click-away-listener": "1.5.0",
    "react-dom": "17.0.1",
    "react-media": "1.10.0",
    "react-relay": "10.1.2",
    "react-router-dom": "5.2.0",
    "react-scripts": "4.0.1",
    "tailwindcss-cli": "0.1.2",
    "typescript": "4.1.3",
    "use-http": "1.0.16",
    "uuid": "8.3.2"
  },
  "scripts": {
    "build:tailwind": "tailwindcss-cli build src/tailwind.css -o src/tailwind.output.css",
    "prestart": "npm run build:tailwind",
    "prebuild": "npm run build:tailwind && npm run relay",
    "start": "concurrently \"nodemon -x npm run relay -e 'ts,tsx,graphql' -i **/__generated__/**\" \"react-scripts start\"",
    "build": "react-scripts build",
    "lint": "eslint --ext .js --ext .tsx --max-warnings 0 src/",
    "test": "react-scripts test",
    "typecheck": "tsc --noEmit -p tsconfig.json",
    "eject": "react-scripts eject",
    "relay": "relay-compiler --src ./src --schema ./shared-graphql/schema.graphql --extensions ts tsx --language typescript"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "jest": {
    "collectCoverageFrom": [
      "**/*.{js,jsx,ts,tsx}",
      "!**/node_modules/**",
      "!**/vendor/**",
      "!**/serviceWorker.ts",
      "!**/*.d.ts",
      "!src/index.tsx",
      "!src/App.tsx",
      "!src/**/*.mocks.ts*"
    ]
  },
  "devDependencies": {
    "@types/faker": "5.1.5",
    "@types/react-relay": "7.0.17",
    "@types/relay-runtime": "10.1.7",
    "@types/relay-test-utils": "6.0.3",
    "babel-plugin-relay": "10.1.2",
    "concurrently": "5.3.0",
    "factory": "https://registry.npmjs.org/factory.ts/-/factory.ts-0.5.1.tgz",
    "faker": "5.1.0",
    "graphql": "15.4.0",
    "nodemon": "2.0.7",
    "relay-compiler": "10.1.2",
    "relay-compiler-language-typescript": "13.0.2",
    "relay-config": "10.1.2",
    "relay-test-utils": "10.1.2"
  }
}
